{
  "project": {
    "version": "1.0.0-beta.21",
    "name": "brainshark-ml"
  },
  "apps": [{
    "root": "src",
    "outDir": "dist",
    "assets": [
      "assets",
      "favicon.ico",
      "assets/libraries/Chart.BarWithHorizontalLine.js",
      "assets/mocks/insights.json",
      "assets/mocks/subtitles.mp4.vtt"
    ],
    "index": "index.html",
    "main": "main.ts",
    "test": "test.ts",
    "tsconfig": "tsconfig.json",
    "prefix": "app",
    "mobile": false,
    "styles": [
      "../node_modules/bootstrap/dist/css/bootstrap.min.css",
      "styles.css",
      "../node_modules/videogular2/fonts/videogular.css",
      "../node_modules/videogular2/fonts/videogular.woff",
      "../node_modules/videogular2/fonts/videogular.svg",
      "../node_modules/videogular2/fonts/videogular.ttf",
      "../node_modules/videogular2/fonts/videogular.eot"

    ],
    "scripts": [
      "../node_modules/jquery/dist/jquery.js",
      "../node_modules/d3/build/d3.js",
      "../node_modules/personality-sunburst-chart/bin/personality-sunburst-chart.standalone.js",
      "../node_modules/chart.js/dist/Chart.min.js",
      "./assets/libraries/Chart.BarWithHorizontalLine.js",
      "../node_modules/wordcloud/src/wordcloud2.js"
    ],
    "environments": {
      "source": "environments/environment.ts",
      "dev": "environments/environment.ts",
      "prod": "environments/environment.prod.ts"
    }
  }],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
